{
  "name": "GitHub Enhancer",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Makes cosmetic changes only to the GitHub website.",
  "homepage_url": "https://github.com/thinkingmedia/ext-github",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://github.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
		"js/jquery/jquery.min.js",
        "src/inject/inject.js"
      ]
    }
  ]
}