<div class="select-menu-modal-holder js-menu-content js-navigation-container" aria-hidden="true">

    <div class="select-menu-modal">
        <div class="select-menu-header">
            <span class="select-menu-title">{{title}}</span>
            <span class="octicon octicon-x js-menu-close"></span>
        </div>

        <div class="select-menu-list">
            {{#Items}}
                <a class="select-menu-item js-navigation-open js-navigation-item{{#selected}} selected{{/selected}}" href="{{url}}">
                    <span class="select-menu-item-icon octicon octicon-check"></span>
                    <span class="select-menu-item-text js-select-button-text">{{title}}</span>
                </a>
            {{/Items}}
        </div>

    </div>
</div>